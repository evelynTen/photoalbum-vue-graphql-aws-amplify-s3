{"ast":null,"code":"export default {\n  mounted() {\n    this.getPhotos();\n  },\n  data: () => ({\n    photos: [],\n    albumName: \"\"\n  }),\n  methods: {\n    async onFileChange(file) {\n      if (!file.target || !file.target.file[0]) {\n        return;\n      }\n      try {\n        await this.$store.dispatch(\"albumInfo/createPhoto\", {\n          file: file.target.file[0],\n          id: this.id\n        });\n        this.getPhotos();\n      } catch (error) {\n        console.log(\"error create photo\", error);\n      }\n    },\n    async getPhotos() {\n      const album = await this.$store.dispatch(\"albumInfo/getAlbum\", this.id);\n      this.photos = album.data.getAlbum.photos.items;\n      this.albumName = album.data.getAlbum.name;\n    }\n  },\n  computed: {\n    id() {\n      return this.$route.params.id;\n    }\n  }\n};","map":{"version":3,"mappings":"AAOI,eAAe;EACXA,OAAO,GAAE;IACL,IAAI,CAACC,SAAS,EAAE;EACpB,CAAC;EACDC,IAAI,EAAE,OAAO;IACTC,MAAM,EAAE,EAAE;IACVC,SAAS,EAAE;EACf,CAAC,CAAC;EACFC,OAAO,EAAE;IACL,MAAMC,YAAY,CAACC,IAAI,EAAC;MACpB,IAAG,CAACA,IAAI,CAACC,MAAK,IAAK,CAACD,IAAI,CAACC,MAAM,CAACD,IAAI,CAAC,CAAC,CAAC,EAAE;QACrC;MACJ;MACA,IAAI;QACA,MAAM,IAAI,CAACE,MAAM,CAACC,QAAQ,CAAC,uBAAuB,EAAE;UAChDH,IAAI,EAAEA,IAAI,CAACC,MAAM,CAACD,IAAI,CAAC,CAAC,CAAC;UACzBI,EAAE,EAAE,IAAI,CAACA;QACb,CAAC,CAAC;QACF,IAAI,CAACV,SAAS,EAAE;MACpB,EAAE,OAAOW,KAAK,EAAE;QACZC,OAAO,CAACC,GAAG,CAAC,oBAAoB,EAAEF,KAAK,CAAC;MAC5C;IACJ,CAAC;IACD,MAAMX,SAAS,GAAE;MACb,MAAMc,KAAI,GAAI,MAAM,IAAI,CAACN,MAAM,CAACC,QAAQ,CAAC,oBAAoB,EAAE,IAAI,CAACC,EAAE;MACtE,IAAI,CAACR,MAAK,GAAIY,KAAK,CAACb,IAAI,CAACc,QAAQ,CAACb,MAAM,CAACc,KAAK;MAC9C,IAAI,CAACb,SAAQ,GAAIW,KAAK,CAACb,IAAI,CAACc,QAAQ,CAACE,IAAI;IAC7C;EACJ,CAAC;EACDC,QAAQ,EAAE;IACNR,EAAE,GAAE;MACA,OAAO,IAAI,CAACS,MAAM,CAACC,MAAM,CAACV,EAAC;IAC/B;EACJ;AACJ","names":["mounted","getPhotos","data","photos","albumName","methods","onFileChange","file","target","$store","dispatch","id","error","console","log","album","getAlbum","items","name","computed","$route","params"],"sourceRoot":"","sources":["/Users/evelynliu/VSCodeProjects/photobook/src/views/AlbumsDetailPage.vue"],"sourcesContent":["<template>\n    <div>\n        <h3>Album Details Page</h3>\n    </div>\n</template>\n\n<script>\n    export default {\n        mounted(){\n            this.getPhotos();\n        },\n        data: () => ({\n            photos: [],\n            albumName: \"\"\n        }),\n        methods: {\n            async onFileChange(file){\n                if(!file.target || !file.target.file[0]) {\n                    return;\n                }\n                try {\n                    await this.$store.dispatch(\"albumInfo/createPhoto\", {\n                        file: file.target.file[0],\n                        id: this.id,\n                    });\n                    this.getPhotos();\n                } catch (error) {\n                    console.log(\"error create photo\", error);\n                }\n            },\n            async getPhotos(){\n                const album = await this.$store.dispatch(\"albumInfo/getAlbum\", this.id)\n                this.photos = album.data.getAlbum.photos.items;\n                this.albumName = album.data.getAlbum.name;\n            }\n        },\n        computed: {\n            id(){\n                return this.$route.params.id\n            }\n        }\n    }\n</script>\n\n<style lang=\"scss\" scoped>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}